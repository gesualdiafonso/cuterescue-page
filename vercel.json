{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://cuterescue-api.onrender.com/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://cuterescue-api.onrender.com/uploads/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}